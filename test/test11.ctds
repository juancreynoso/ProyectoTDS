Program
{
    void assert(integer n, integer m) extern;
    integer inc(integer x) {
        return x + 1;
    }

    integer sum (integer a, integer b) {
        return a+b;
    }

    integer main() {
        integer x = 0;
        x = sum(inc(3),inc(1));
        assert(x, 6);
        return x;
    }
}
