Program
{
    void assert(integer n, integer m) extern;
    integer fact(integer n) {
        if (n == 0 || n == 1) then {
            return 1;
        } else {
            if (n > 1) then {
                return n * fact(n-1);
            }
        }
    }
    integer print(integer x) extern;
    integer main() {
        integer x = 5;
        x = fact(x);
        assert(x, 120);
        print(x);
        return x;
    }
}
